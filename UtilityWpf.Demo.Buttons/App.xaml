<Application x:Class="UtilityWpf.Demo.Buttons.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns:infra="clr-namespace:UtilityWpf.Demo.Common.ViewModel;assembly=UtilityWpf.Demo.Common"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate DataType="{x:Type infra:ButtonViewModel}">
                <Ellipse Fill="Green" Height="40" Width="40"></Ellipse>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>