<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="UniversalWPF" Version="0.9.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="CornerPanelView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="SidePanelView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="UniformGridPanelView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="EdgeLegacyPanelView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="RegionPanel3View.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="RegionPanel2View.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="RegionPanelView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="CornerPanelView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="SidePanelView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="UniformGridPanelView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
