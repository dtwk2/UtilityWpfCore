<Window x:Class="UtilityWpf.Demo.Hybrid.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UtilityWpf.Demo.Hybrid" 
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <TabControl>
        <TabItem Header="Ticks">
            <local:TicksUserControl />
        </TabItem>

        <TabItem Header="Group">
            <local:GroupsUserControl />
        </TabItem>
        
        <TabItem Header="Notes">
            <local:NotesUserControl />
        </TabItem>     

        <TabItem Header="Persist">
            <local:MeasurementsUserControl />
        </TabItem>
    </TabControl>
</Window>
