<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.0.20.0</AssemblyVersion>
    <FileVersion>1.0.20.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\UtilityWpf.Demo.Animation.View\UtilityWpf.Demo.Animation.View.csproj" />
    <ProjectReference Include="..\UtilityWpf.Demo.Panels\UtilityWpf.Demo.Panels.View.csproj" />
    <ProjectReference Include="..\UtilityWpf.Demo.View\UtilityWpf.Demo.View.csproj" />
    <ProjectReference Include="..\UtilityWpf.TestData\UtilityWpf.TestData.csproj" />
  </ItemGroup>

  <ItemGroup>
    <ApplicationDefinition Update="App.xaml">
      <SubType>Designer</SubType>
      <Generator>MSBuild:Compile</Generator>
    </ApplicationDefinition>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Themes\ViewHostControl.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>


</Project>