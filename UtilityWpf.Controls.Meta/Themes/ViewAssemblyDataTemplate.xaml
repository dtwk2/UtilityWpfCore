<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:util="http://schemas.utility.com/utilitywpf">

    <!-- Used by ViewHostControl-->

    <DataTemplate DataType="{x:Type util:AssemblyKeyValue}">

        <TextBlock MinHeight="20" MinWidth="100"
                            Margin="32,0,32,0"
                            FontSize="14"
                            Text="{Binding Key}" />
    </DataTemplate>
</ResourceDictionary>